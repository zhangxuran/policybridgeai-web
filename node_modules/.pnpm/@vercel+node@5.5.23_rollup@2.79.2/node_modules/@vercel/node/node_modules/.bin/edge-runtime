#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/shadcn-ui/node_modules/.pnpm/edge-runtime@2.5.9/node_modules/edge-runtime/dist/cli/node_modules:/home/ubuntu/shadcn-ui/node_modules/.pnpm/edge-runtime@2.5.9/node_modules/edge-runtime/dist/node_modules:/home/ubuntu/shadcn-ui/node_modules/.pnpm/edge-runtime@2.5.9/node_modules/edge-runtime/node_modules:/home/ubuntu/shadcn-ui/node_modules/.pnpm/edge-runtime@2.5.9/node_modules:/home/ubuntu/shadcn-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/shadcn-ui/node_modules/.pnpm/edge-runtime@2.5.9/node_modules/edge-runtime/dist/cli/node_modules:/home/ubuntu/shadcn-ui/node_modules/.pnpm/edge-runtime@2.5.9/node_modules/edge-runtime/dist/node_modules:/home/ubuntu/shadcn-ui/node_modules/.pnpm/edge-runtime@2.5.9/node_modules/edge-runtime/node_modules:/home/ubuntu/shadcn-ui/node_modules/.pnpm/edge-runtime@2.5.9/node_modules:/home/ubuntu/shadcn-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../edge-runtime@2.5.9/node_modules/edge-runtime/dist/cli/index.js" "$@"
else
  exec node  "$basedir/../../../../../../edge-runtime@2.5.9/node_modules/edge-runtime/dist/cli/index.js" "$@"
fi
