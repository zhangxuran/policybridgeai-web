#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/ubuntu/shadcn-ui/node_modules/.pnpm/srvx@0.8.9/node_modules/srvx/bin/node_modules:/home/ubuntu/shadcn-ui/node_modules/.pnpm/srvx@0.8.9/node_modules/srvx/node_modules:/home/ubuntu/shadcn-ui/node_modules/.pnpm/srvx@0.8.9/node_modules:/home/ubuntu/shadcn-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/ubuntu/shadcn-ui/node_modules/.pnpm/srvx@0.8.9/node_modules/srvx/bin/node_modules:/home/ubuntu/shadcn-ui/node_modules/.pnpm/srvx@0.8.9/node_modules/srvx/node_modules:/home/ubuntu/shadcn-ui/node_modules/.pnpm/srvx@0.8.9/node_modules:/home/ubuntu/shadcn-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../srvx@0.8.9/node_modules/srvx/bin/srvx.mjs" "$@"
else
  exec node  "$basedir/../../../../../../srvx@0.8.9/node_modules/srvx/bin/srvx.mjs" "$@"
fi
